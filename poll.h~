#ifndef pollhead
#define pollhead

////////////////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////
typedef struct  url_info u_info;
typedef struct url_info* u_info_point;
struct url_info{
		int total_url;
		int total_img;
		int total_size;
		char* type;
		double Rtime;
		double Ctime;
		
};

////////////////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////		
typedef struct  poll_str poll;
typedef struct poll_str* poll_point;
struct poll_str{
		struct pollfd fd[2];
		};
////////////////////////////////////////////////////////////////////////////////////////////////////////////

poll_point create_ptable(int table_size);

void print_poll(u_info pack);

#endif /*pollhead*/


