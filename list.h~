typedef struct list_node l_node;
typedef struct list_node* l_node_point;
struct list_node{ 
	char* url;
	l_node_point next;		
};
////////////////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////
l_node_point create_list(char*  data);
////////////////////////////////////////////////////////////////////////////////////////////////////////////
l_node_point insert(l_node_point pointer, char* data );
////////////////////////////////////////////////////////////////////////////////////////////////////////////
void print_list(l_node_point start);
////////////////////////////////////////////////////////////////////////////////////////////////////////////
void print_info(char* data);
////////////////////////////////////////////////////////////////////////////////////////////////////////////
void destroy_node( l_node_point node);
////////////////////////////////////////////////////////////////////////////////////////////////////////////
void delete_list(l_node_point pointer);
////////////////////////////////////////////////////////////////////////////////////////////////////////////

#endif /*listhead*/


