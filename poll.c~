#include <stdio.h>
#include <stdlib.h>
#include "poll.h"
////////////////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////
poll_point create_ptable(int table_size)
{

    	poll_point poll_table;
	//printf("Created Poll table with size: %d\n",table_size);
    	poll_table= malloc( table_size*sizeof(poll) );
    	return poll_table;
}
////////////////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////
void print_poll(u_info pack){
	printf("Printing URL INFO STRUCT!\nTotal Url: [%d]\nTotal Size: [%d]\nType: [%s]\nRun Time: [%lf]\nCPU Time: [%lf]\n",pack.total_url,pack.total_img,pack.type,pack.Rtime,pack.Ctime);

}

